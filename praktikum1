#algoritma1
#input nilai awal yang merupakan angka yang paling besar
x_awal1 = 10000.0

#input nilai selanjutnya yang merupakan angka yang kecil
x_selanjutnya1 = 0.0001

#melakukan perulangan angka 0.0001 sebnayk 5 kali
for _ in range (5):
  x_awal1 += x_selanjutnya1

#cetak hasil
print(f"Hasil Algoritma 1 : {x_awal1}")

#algoritma 2
total2 = 0.0
#input nilai awal yang merupakan angka kecil
x_awal2 = 0.0001

#perulangan angka 0.0001 sebanyak 5 kali
for _ in range (5):
  total2 += x_awal2

#input nilai selanjutnya yang merupakan angka yang besar
total2 +=10000.0
#cetak hasil
print (f"Hasil Algoritma 2 : {total2}")
